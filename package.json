{
  "name": "protobuf-table",
  "version": "1.2.3",
  "description": "A dynamic protobuf implementation for structured table data",
  "main": "javascript/dist/pbTable.js",
  "scripts": {
    "build-proto": "node javascript/scripts/build-proto.js",
    "build-dev": "npm run build-proto && babel javascript/src/pbTable.js -o javascript/dist/pbTable.js",
    "build-min": "cross-env BABEL_ENV=production babel javascript/src/pbTable.js -o javascript/dist/pbTable-min.js",
    "build": "npm run build-dev && npm run build-min",
    "test": "node javascript/test/index.js",
    "prebuild": "npm run build-proto"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mappazzo/protobuf-table.git"
  },
  "keywords": [
    "protobuf",
    "structured",
    "data",
    "compression",
    "binary",
    "big",
    "javascript",
    "js",
    "node"
  ],
  "author": "Kelly Norris",
  "license": "Beerware",
  "bugs": {
    "url": "https://github.com/mappazzo/protobuf-table/issues"
  },
  "homepage": "https://github.com/mappazzo/protobuf-table#readme",
  "dependencies": {
    "protobufjs": "^6.8.0"
  },
  "devDependencies": {
    "autoprefixer": "^6",
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.2.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-minify": "^0.2.0",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0"
  }
}
